<h1>New User</h1>

<%= form_for(@user) do |form| %>
    <%= form.text_field :first_name %>
    <%= form.text_field :last_name %>
    <%= form.text_field :email %>
    <%= form.text_field :password %>
    <%= form.submit "Submit" %>
      <% end %>

<%= render 'form', user: @user %>

<%= link_to 'Back', users_path %>
